{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.lusochem.com",
  "dependencies": {
    "@ebay/browserslist-config": "^1",
    "@ebay/skin": "^16.9.0",
    "@ebay/ui-core-react": "^5.11.0",
    "@tinymce/tinymce-react": "^4.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.10",
    "antd": "^5.11.5",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "font-awesome": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-modal-video": "^2.0.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "react-window": "^1.8.10",
    "typescript": "^5.4.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ignore-path .eslintignore --ext js,jsx,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --ignore-path .eslintignore --ext js,jsx,tsx --report-unused-disable-directives --fix",
    "prettier": "npx prettier . --write",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "npx prettier --write",
      "git add"
    ],
    "*.{js,jsx}": "npm run lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lozad": "^1.16.0",
    "prettier": "3.1.1"
  }
}